<p id="notice"><%= notice %></p>

<h1>Docfiles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @docfiles.each do |docfile| %>
      <tr>
        <td><%= docfile.title %></td>
        <td><%= link_to 'Show', docfile %></td>
        <td><%= link_to 'Edit', edit_docfile_path(docfile) %></td>
        <td><%= link_to 'Destroy', docfile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Docfile', new_docfile_path %>
